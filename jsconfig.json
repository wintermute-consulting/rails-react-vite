{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "baseUrl": "app/frontend",
    "paths": {
      "~/*": [
        "./*"
      ]
    },
    "jsx": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "app/frontend"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}